{
  "name": "pageaccessvalidator",
  "version": "1.0.0",
  "description": "Testes de acesso as paginas do projeto",
  "main": "index.js",
  "scripts": {
    "pretest": "mkdir -p cypress/videos cypress/reports",
    "test": "npx cypress open"
  },
  "author": "Jorge Luiz Cardoso",
  "license": "MIT",
  "devDependencies": {
    "cypress-mochawesome-reporter": "^3.7.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "video": true,
  "videoUploadOnPasses": false,
  "videosFolder": "cypress/videos",
  "reporter": "mochawesome",
  "reporterOptions": {
    "reportDir": "cypress/reports",
    "overwrite": false,
    "html": false,
    "json": true
  },
  "dependencies": {
    "cypress": "^12.17.4"
  }
}
